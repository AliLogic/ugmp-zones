// Functions

returnZoneName(Float: x, Float: y, Float: z) {

	new
		zoneName[32]
	;

	getZoneName(zoneName, sizeof zoneName, x, y, z);
	return zoneName;
}

getZoneName(zoneName[], zoneNameSize = sizeof zoneName, Float: posX, Float: posY, Float: posZ) {
	
	for (new i = 0; i != sizeof g_ZonesData; i ++) {
		if ((x >= g_ZonesData[i][E_ZONE_AREA][0] && x <= g_ZonesData[i][E_ZONE_AREA][3]) &&
			(y >= g_ZonesData[i][E_ZONE_AREA][1] && y <= g_ZonesData[i][E_ZONE_AREA][4]) &&
			(z >= g_ZonesData[i][E_ZONE_AREA][2] && z <= g_ZonesData[i][E_ZONE_AREA][5])) {
			strunpack(zoneName, g_ZonesData[i][E_ZONE_NAME], size);
			return 1;
		}
	}

	return 0;
}
